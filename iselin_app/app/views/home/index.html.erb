<div id="page">
	<div id="elementWrapper">
		<%= render "menu" %>
		<div id="content">
			<div id="contentwrapper">
				<script type="text/javascript">
				$(function(){
					$("#slides").slides({
						effect: 'slide, fade',
					});
				});
				</script>
				<div id="slidewrapper">
					<div id="frame">
						<div id="slides">
							<div class="slides_container">
								<% @images.each do |image|%>
								<div>
									<%= image_tag(image, :alt => "image", :style => "margin-top:") %>
								</div>
								<% end %>
							</div>
							<a href="#" class="prev"><%= image_tag("arrow-prev.png", :alt => "Arrow Prev", :width => "24", :height => "43") %></a>
							<a href="#" class="next"><%= image_tag("arrow-next.png", :alt => "Arrow Next", :width => "24", :height => "43") %></a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div id="footer">&nbsp;</div>
</div>